6)
  int main(int argc, char *argv[])
  {
    printf("Olá, %s!\n", argv[1]);
    printf("Número de argumentos = %d\n", argc);
    return 0;
  }
  
7)
  int main(int argc, char *argv[])
  {
    int i;
    printf("\n");
    for(i=1;i<=(argc-1);i++)
    {
      printf("%s ", argv[i]);
    }
    printf("\n");
    return 0;
  }
  
8)
  int Num_Caracs(char *string)
  {
    int i=0;
    int num=0;
    int indice=0;
    while(i==0)
    {
      if(string[indice] == '\0')
      {
        i=1;
      }
      else
      {
        num+=1;
        i=0;
      }
      indice+=1;
    }
    return num;
  }
  
9)
  int main(int argc, char *argv[])
  {
    int i;
    int num;
    printf("\n");
    for(i=0;i<=(argc-1);i++)
    {
      num = Num_Caracs(argv[i]);
      printf("%s / Número de caracteres: %d\n", argv[i], num);
    }
    printf("\n");
    return 0;
  }



 
